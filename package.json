{
  "name": "@knowark/injectarkjs",
  "version": "0.2.0",
  "description": "Dependency Injector for Javascript",
  "main": "index.js",
  "scripts": {
    "clear": "rm -rf node_modules/",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text babel-node tests/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knowark/injectarkjs.git"
  },
  "keywords": [
    "injection",
    "dependency"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/run.js",
      "build"
    ]
  },
  "author": "Nubark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knowark/injectarkjs/issues"
  },
  "homepage": "https://github.com/knowark/injectarkjs#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.2.0",
    "jasmine": "^3.3.1",
    "nyc": "^13.2.0"
  }
}
